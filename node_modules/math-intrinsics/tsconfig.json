{
  "extends": "@ljharb/tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": false,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}